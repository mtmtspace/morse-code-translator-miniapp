    "web audio"
  ],
  "license": "MIT",
  "homepage": "https://morsecodetranslator.com",
  "author": "Burak Ã–zdemir",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozdemirburak/morse-code-translator.git"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.9",
    "happy-dom": "^20.0.10",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
